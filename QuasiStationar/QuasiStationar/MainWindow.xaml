<Window x:Class="QuasiStationar.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Singular Value Decomposition - QuasiStationar" MinHeight="450" MinWidth="750" Height="450" Width="750">
    <Grid>
        <Button Content="Run" Height="23" HorizontalAlignment="Right" Margin="10,10,90,10" x:Name="buttonRun" VerticalAlignment="Bottom" Width="75" Click="ButtonRunClick" />
        <Button Content="Open" Height="23" HorizontalAlignment="Right" Margin="10" x:Name="buttonOpen" VerticalAlignment="Bottom" Width="75" Click="ButtonOpenClick" />
        <TextBox Height="350" Margin="10,10,10,0" x:Name="textBox" VerticalAlignment="Top" TextWrapping="Wrap" VerticalScrollBarVisibility="Auto" AcceptsReturn="True" IsReadOnly="True" />
        <ProgressBar Height="23" HorizontalAlignment="Left" Margin="10" x:Name="progressBar" VerticalAlignment="Bottom" Width="400" />
        <ComboBox Height="23" HorizontalAlignment="Left" Margin="416,378,0,0" x:Name="comboBox" VerticalAlignment="Top" Width="141" />
    </Grid>
</Window>
